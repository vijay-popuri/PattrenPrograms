package simplePattrens;

public class Patt49 {
	public static void main(String[] args) {
		int n=5;
		for(int i=1;i<=n;i++) {
			int p=i;
			for(int j=1;j<=i;j++) {
				System.out.print(p+" ");
				p++;
			}
			System.out.println();
		}
//	/	System.out.println(5=(5-4++));
		int b=5,a=4;
		b=5-a++;
	}

}
