package jdBasic;

public class LinearSearch
{
	public static void main(String[] args) {
		int flag=0;
		int ele=9;
		int a[]= {2,3,5,6,8,9,8765,423,67};
		for(int i=0;i<a.length;i++) {
			if(a[i]==ele) {
				System.out.println("fountd at"+i);
				flag=1;
						break;
			}
		}
		if(flag==0) {
			System.out.println("element not found");
		}
		
	}

}
